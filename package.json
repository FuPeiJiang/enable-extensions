{
	"name": "activate-extensions",
	"displayName": "activate extensions",
	"description": "",
	"publisher": "FuPeiJiang",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"main": "./extension.js",
	"extensionDependencies": [],
	"repository": {
		"type": "git"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*",
		"onCommand:activate-extensions.helloWorld"
	],
	"contributes": {
		"commands": [
			{
				"command": "activate-extensions.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"ext": "webpack --mode production && vsce package -o activate-extensions.vsix && code --install-extension activate-extensions.vsix",
		"vsix": "webpack --mode production && vsce package -o activate-extensions.vsix",
		"webpack": "webpack --mode production",
		"package": "vsce package -o activate-extensions.vsix",
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/vscode": "^1.52.0",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"ts-loader": "^8.0.12",
		"typescript": "^3.8.3",
		"vsce": "^1.83.0",
		"vscode-test": "^1.4.0",
		"webpack": "^5.11.0",
		"webpack-cli": "^4.3.0"
	}
}
